                        + '직접 대기해제 해주십시오.', CDMSE);
            end else
              if GB_JON_FINISHMSG then GMessagebox(MDXCI_0006, CDMSI);
					end else
					begin
						if GB_JON_FINISHMSG then GMessagebox(MDXCI_0006, CDMSI);
					end;
				end;

				Application.ProcessMessages;

				if (cxtJoinNum.Hint = '5') and ( GB_ResToJoin_USE ) and (bJoinBtn) then  //대기오더 수정시 접수로 변경 사용시 20190618 KHS
				begin
