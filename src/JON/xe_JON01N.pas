				if Application.MessageBox(PChar(msg), CDMSI, MB_YESNO+MB_ICONQUESTION+MB_DEFBUTTON1) = IDYES then
				begin
					gsUpdateCallBellID := gsCuCallBellID;       //고객정보의 CallBellID로 오더정보변경
					gsUpdateCallBellStatus := pnlCallBell.Hint;
