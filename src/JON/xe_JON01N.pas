        iLeft := Self.Left
      end;

      EnvFile.WriteInteger('WinPos', 'Jon01Left', iLeft);
      EnvFile.WriteInteger('WinPos', 'Jon01Top' , Self.Top );
    end;

    Frm_Main.iJonLeft := Self.Left;
    Frm_Main.iJonTop  := Self.Top;

    GMessagebox('설정 완료', CDMSI);
