        begin
        end else
        begin
          BtnSmartRate.Click;
          if curRate.CanFocus then curRate.SetFocus;
        end;
      end;
      outPriceInfo := '스마트요금 (' + oType + ') ] ' + FormatFloat('#,', iSmartRate);
      Result := iSmartRate;
    end;
  end;

  outPriceInfo := '스마트요금 (' + oType + ') ] ' + FormatFloat('#,', iSmartRate);
  Result := iSmartRate;
