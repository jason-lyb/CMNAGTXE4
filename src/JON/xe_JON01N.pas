        end else
        if (cbbPayMethod.Text = PAY_METHOD_BUSAN2K) and (GS_PRJ_AREA = 'O') AND ((GT_USERIF.ShareNo = 'G03') or (pJON01Dock.HDNO = 'A100')) then //부산연합하트2K 20140221 KHS
