{
SELECT (SELECT HD_NAME || '(' || CU.HD_NO || ')' FROM CDMS_HEAD WHERE HD_NO = CU.HD_NO),
       (SELECT BR_NAME || '(' || CU.BR_NO || ')' FROM CDMS_BRANCH WHERE BR_NO = CU.BR_NO),
       CU.KEY_NUMBER,
       DECODE (CU.CU_TYPE,'1', DECODE (CU.CALLBELL_STATUS, '1', '콜벨업소', '업소'),'3', '법인','4', '불량','일반'),
       (SELECT TO_CHAR(WM_CONCAT(CT.CU_TEL || '/' || CT.CU_SMSYN)) FROM CDMS_CUSTOMER_TEL CT WHERE CT.CU_SEQ = CU.CU_SEQ) TEL,
       CU.CMP_NM,
       CU.CMP_NO,
			 (CU.CU_ENDCNT + CU.CU_CANCELCNT),
       CU.CU_ENDCNT,
       (SELECT LEVEL_NAME FROM CDMS_CUST_LEVEL WHERE SEQ = CU.CU_LEVEL_CD),
       CU.CB_CODE, CU.CU_INFO, CU.CU_PDAINFO,
       CU.CU_AREA5 || '/' || CU.CU_AREA || ' ' || CU.CU_AREA2 || ' ' || CU.CU_AREA3,
			 CU.CU_EDAREA5 || '/' || CU.CU_EDAREA || ' ' || CU.CU_EDAREA2 || ' ' || CU.CU_EDAREA3,
       CU.CU_SEQ, CU.BR_NO, CU.CU_MILEAGE, CU.CU_LAST_END,
       (SELECT cu_virtual_tel FROM CDMS_CUSTOMER_TEL WHERE cu_seq = CU.cu_seq AND cu_virtual_yn = 'y') virtual_tel,
       (SELECT ALLOT_TIME FROM VIRTUAL_NUMBER_0507 WHERE v_phone = (SELECT cu_virtual_tel FROM CDMS_CUSTOMER_TEL WHERE cu_seq = CU.cu_seq AND cu_virtual_yn = 'y') AND c_type = '3'),
       TO_CHAR (APP.LAST_REG_DATE, 'YYYYMMDD'), TO_CHAR (APP.LAST_DEL_DATE, 'YYYYMMDD'), APP.LAST_FINISH_DATE,
       DECODE (CU.CU_OUTBOUND, '1', '동의', '2', '거부', '3', '문자', '') CU_OUTBOUND,
       CU.HD_NO, CU.CALLBEL_REG_DATE, TO_CHAR (CU.IN_DATE, 'YYYYMMDD'), CU.CU_EXPIRE_MILEAGE,
       ADD_MONTHS (TRUNC (SYSDATE, 'MM'), 1) + 9 / 24 AS MLG_EXPIRE_DATE, CB_POSITION_NAME
	FROM CDMS_CUSTOMER CU, (SELECT LAST_REG_DATE, LAST_DEL_DATE, LAST_FINISH_DATE, CU_SEQ, del_yn FROM CDMS_APP_USER) APP
 WHERE  CU.HD_NO = '%s' %s
}

