								 AND WK_HEAD = '%s' %S AND A.CONF_WK_SABUN = B.WK_SABUN) ORD,
				 CDMS_A01_CHARGE CHA, CDMS_BRANCH BRCH
	 WHERE ORD.CONF_SLIP = CHA.CONF_SLIP(+) AND ORD.WK_BR = BRCH.BR_NO
GROUP BY BRCH.BR_NAME, ORD.WK_BR, ORD.WK_SABUN, ORD.WK_HP ORDER BY ORD.WK_SABUN}

