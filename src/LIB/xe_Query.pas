				 SUM(NVL(DECODE(ORD.CONF_STATUS, 1, DECODE(ORD.ORDER_HD, ORD.WK_HD, 0, 1), 0), 0)) TASA_ING,
				 SUM(NVL(DECODE(ORD.CONF_STATUS, 8, DECODE(ORD.ORDER_HD, ORD.WK_HD, 1, 0), 0), 0)) JASA_CENCLE,
				 SUM(NVL(DECODE(ORD.CONF_STATUS, 8, DECODE(ORD.ORDER_HD, ORD.WK_HD, 0, 1), 0), 0)) TASA_CENCLE,
				 SUM(NVL(DECODE(ORD.CONF_STATUS, 2, ORD.CHARGE,  1, ORD.CHARGE,  8, 0), 0)) DRV_CHARGE,
				 SUM(NVL(DECODE(ORD.ORDER_HD, ORD.WK_HD, CHA.COMMISSION, 0), 0)) JASA_CMS,
				 SUM(NVL(DECODE(ORD.ORDER_HD, ORD.WK_HD, 0, (CHA.COMMISSION - CHA.CALL_BUSINESS - CHA.CALL_ACCEPT)), 0)) TASA_CMS, ORD.WK_HP
