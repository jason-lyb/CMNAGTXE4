{
	SELECT *
    FROM (SELECT TO_CHAR (A.IN_DATE, 'MM/DD-HH24:MI:SS') IN_MMDDHHMI,
                 A.CONF_STATUS AS ST_NM,
                 DECODE ( (SELECT 'COUNSEL'
                             FROM CDMS_MEMBER
                            WHERE MEM_ID = A.IN_ID),
                         'COUNSEL', (SELECT MEM_NAME
                                       FROM CDMS_MEMBER
                                      WHERE MEM_ID = A.IN_ID),
                         (SELECT WK_NAME
                            FROM CDMS_WORKER
                           WHERE WK_SABUN = A.IN_ID))
                    COUNSEL_NAME,
                 DECODE (
                    (SELECT 'COUNSEL'
                       FROM CDMS_MEMBER
                      WHERE MEM_ID = A.IN_ID),
                    'COUNSEL', (SELECT Y.BR_NAME
                                  FROM CDMS_MEMBER X, CDMS_BRANCH Y
                                 WHERE X.BR_NO = Y.BR_NO AND X.MEM_ID = A.IN_ID),
                    (SELECT Y.BR_NAME
                       FROM CDMS_WORKER X, CDMS_BRANCH Y
                      WHERE X.BR_NO = Y.BR_NO AND X.WK_SABUN = A.IN_ID))
                    SOSOK,
                 DECODE (
                    (SELECT 'COUNSEL'
                       FROM CDMS_MEMBER
                      WHERE MEM_ID = A.IN_ID),
                    'COUNSEL', (SELECT Y.BR_TEL
                                  FROM CDMS_MEMBER X, CDMS_BRANCH Y
                                 WHERE X.BR_NO = Y.BR_NO AND X.MEM_ID = A.IN_ID),
                    (SELECT Y.BR_TEL
                       FROM CDMS_WORKER X, CDMS_BRANCH Y
                      WHERE X.BR_NO = Y.BR_NO AND X.WK_SABUN = A.IN_ID))
                    SOSOK_TEL,
                 A.CONF_WK_SABUN,
                 (SELECT WK_NAME
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN)
                    AS WK_NAME,
                 (SELECT WK_HP
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN)
                    AS WK_HP,
                 (SELECT WK_PHONE
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN)
                    AS WK_PHONE,
                 DECODE (A.CONF_STATUS,
                         '7', '배차취소',
                         '8', (SELECT CONF_CREASON
																 FROM %s
                                WHERE CONF_SLIP = A.CONF_SLIP),
                         '')
                    CREASON,
                 MODIFIED,
                 A.CONF_AREA5 || '/' || A.CONF_AREA3 CONF_AREA,
                 A.CONF_EDAREA5 || '/' || A.CONF_EDAREA3 CONF_EDAREA,
                 A.CONF_CHARGE,
                 (SELECT BR_WK_NUM
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN),
                 (SELECT BR_NO
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN),
                 (SELECT WK_VHP
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN)
                    AS WK_VHP,
                 (SELECT WK_VPHONE
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN)
                    AS WK_VPHONE,
                 (SELECT HD_NO
                    FROM CDMS_WORKER X
                   WHERE A.CONF_WK_SABUN = X.WK_SABUN)
            FROM CDMS_A01_HIS A
           WHERE A.CONF_SLIP = '%s' AND A.CONF_STATUS NOT IN ('E', 'L', 'U')
          UNION ALL
          SELECT TO_CHAR (A.IN_DATE, 'MM/DD-HH24:MI:SS') IN_MMDDHHMI,
                 'S' AS ST_NM,
                 '' AS COUNSEL_NAME,
                 '' AS SOSOK,
                 '' AS SOSOK_TEL,
                 '' AS CONF_WK_SABUN,
                 '' AS WK_NAME,
                 '' AS WK_HP,
                 '' AS WK_PHONE,
                 '' AS CREASON,
                 A.MSG AS MODIFIED,
                 '' AS CONF_AREA,
                 '' AS CONF_EDAREA,
                 0 AS CONF_CHARGE,
                 '' AS BR_WK_NUM,
                 '' AS BR_NO,
                 '' AS WK_VHP,
                 '' AS WK_VPHONE,
								 '' AS HD_NO
						FROM SMSLIST A
					 WHERE A.CONF_SLIP = '%s' AND KEY_NUMBER IS NOT NULL)
ORDER BY IN_MMDDHHMI DESC, ST_NM

}
